<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/patientColumn"
    android:layout_width="@dimen/patient_columns_width"
    android:layout_height="@dimen/patient_columns_height"
    android:background="#00000000"
    tools:context=".view.MainActivity">

    <TextView
        android:id="@+id/patientHeader"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="P-X"
        android:textColor="@color/temp"
        android:textSize="@dimen/patient_columns_size"
        android:gravity="center_horizontal"
        android:background="#00000000"/>
</RelativeLayout>
